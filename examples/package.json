	"license": "ISC",
	"packageManager": "pnpm@10.28.0",
	"dependencies": {
		"@buddy-works/sandbox-sdk": "file:.."
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.11",
		"@dotenvx/dotenvx": "catalog:",
		"@tsconfig/node20": "catalog:",
		"@tsconfig/strictest": "catalog:",
